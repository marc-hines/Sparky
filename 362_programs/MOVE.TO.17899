OPEN 'ACDB/GL.PCL' TO GL.PCL.FILE ELSE STOP
SELECT GL.PCL.FILE
LOOP WHILE READNEXT ID DO
  ID1 = FIELD(ID,'*',1)
  ID2 = FIELD(ID,'*',2)
  ID3 = FIELD(ID,'*',3)
  ID4 = FIELD(ID,'*',4)
  NEW.ID = ID1:'*':ID2:'*':ID3:'*17899'
  IF ID4 EQ '18264' THEN
    READ GL.PCL.REC FROM GL.PCL.FILE,ID THEN
      WRITE GL.PCL.REC ON GL.PCL.FILE,NEW.ID
      DELETE GL.PCL.FILE,ID
    END
  END
REPEAT